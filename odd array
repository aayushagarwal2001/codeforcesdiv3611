#include <iostream>
#include <vector>
using namespace std;
class react
{public:
vector<int> arr;
};
int main()
{int r,i,j;
cin>>r;
 class react a[r];

for(i=0;i<r;i++)
{int n,even=0,odd=0,d,f;
    cin>>n;
    for(j=0;j<n;j++)
    {int c;
    cin>>c;
    a[i].arr.push_back(c);
    }
    for(j=0;j<n;j++)
    {if(a[i].arr[j]%2==0)
    {even++;}
    else
    {odd++;}
    }

if(odd%2==0&&odd!=0)
{for(j=0;j<n;j++)
{if(a[i].arr[j]%2!=0)
{d=j;
break;}}
for(j=0;j<n;j++)
{if(a[i].arr[j]%2==0)
{f=j;
break;
}
}


a[i].arr[f]=a[i].arr[d];

    int sum=0;
    for(j=0;j<n;j++)
    {
    sum=sum+a[i].arr[j];}
    if(sum%2==0)
    {cout<<"NO"<<endl;
    }
    else
        {cout<<"YES"<<endl;}
        
}  


   else 
    { int sum=0;
    for(j=0;j<n;j++)
    {
    sum=sum+a[i].arr[j];}
    if(sum%2==0)
    {cout<<"NO"<<endl;
    }
    else
        {cout<<"YES"<<endl;}
        }
        
}


return 0;
}
